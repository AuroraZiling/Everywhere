<shadui:Window
  x:Class="Everywhere.Views.TransientWindow" xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:markupExtensions="clr-namespace:Everywhere.MarkupExtensions"
  xmlns:shadui="clr-namespace:ShadUI.Controls;assembly=ShadUI" Title="Everywhere"
  MinWidth="640" Focusable="True"
  Icon="avares://Everywhere/Assets/Everywhere.png" TitleFontWeight="SemiBold"
  TransparencyLevelHint="Mica,AcrylicBlur,Blur,None">

  <shadui:Window.Hosts>
    <shadui:DialogHost Manager="{markupExtensions:ServiceLocator Type=shadui:DialogManager}"/>
    <shadui:ToastHost Manager="{markupExtensions:ServiceLocator Type=shadui:ToastManager}"/>
  </shadui:Window.Hosts>
</shadui:Window>
