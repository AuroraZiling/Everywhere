<Application
    x:Class="Everywhere.App" xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:suki="https://github.com/kikipoulet/SukiUI">
    <Application.Styles>
        <suki:SukiTheme ThemeColor="Blue"/>
        <StyleInclude Source="avares://IconPacks.Avalonia.Material/Material.axaml"/>
        <StyleInclude Source="avares://Everywhere/Assets/Styles.axaml"/>
    </Application.Styles>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <MergeResourceInclude Source="avares://Everywhere/Assets/Resources.axaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>